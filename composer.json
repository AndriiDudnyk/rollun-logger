{
  "name": "rollun-com/rollun-logger",
  "description": "Logger",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "avz-cmf",
      "email": "email@example.com"
    },
    {
      "name": "victorynox",
      "email": "it.proffesor02@gmail.com"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "zendframework/zend-servicemanager": "^3.0",
    "zendframework/zend-stdlib": "^3.0.1",
    "zendframework/zend-stratigility": "^1.1",
    "zendframework/zend-diactoros": "^1.1",
    "zendframework/zend-config": "^2.6",
    "phpunit/phpunit": "^4.0",
    "rollun-com/rollun-dic": "master-dev",
    "rollun-com/rollun-installer": "master-dev",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "rollun\\logger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "rollun\\test\\logger\\": "tests/src"
    }
  },
  "scripts": {
    "lib-install": "rollun\\installer\\Command::install",
    "lib-uninstall": "rollun\\installer\\Command::uninstall",
    "lib-reinstall": "rollun\\installer\\Command::reinstall"
  }
}
